{
    "presets": ["next/babel", ["@babel/env", { "targets": { "node": 6 } }], "@babel/react"],
    "plugins": [
        [
            "styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false
            }
        ],
        "lodash"
    ],
    "env": {
        "production": {
            "plugins": ["transform-remove-console"]
        }
    }
}
